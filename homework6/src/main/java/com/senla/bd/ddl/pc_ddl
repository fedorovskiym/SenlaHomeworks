-- public.pc определение

-- Drop table

-- DROP TABLE public.pc;

CREATE TABLE public.pc (
	code int4 NOT NULL,
	model varchar(50) NOT NULL,
	speed int2 NOT NULL,
	ram int2 NOT NULL,
	hd float4 NOT NULL,
	cd varchar(10) NOT NULL,
	price money NULL,
	CONSTRAINT pc_pkey PRIMARY KEY (code)
);


-- public.pc внешние включи

ALTER TABLE public.pc ADD CONSTRAINT fk_model FOREIGN KEY (model) REFERENCES public.product(model);